<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/css/normalize.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/css/themify-icons.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/css/flag-icon.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/css/cs-skin-elastic.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/Dashboard/")assets/scss/style.css">
    <link href="@Url.Content("~/Content/Dashboard/")assets/css/lib/vector-map/jqvmap.min.css" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/vendor/jquery-2.1.4.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/plugins.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/main.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/lib/chart-js/Chart.bundle.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/dashboard.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/widgets.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/lib/vector-map/jquery.vmap.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/lib/vector-map/jquery.vmap.min.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/lib/vector-map/jquery.vmap.sampledata.js"></script>
    <script src="@Url.Content("~/Content/Dashboard/")assets/js/lib/vector-map/country/jquery.vmap.world.js"></script>
    <script>
        ( function ( $ ) {
            "use strict";

            jQuery( '#vmap' ).vectorMap( {
                map: 'world_en',
                backgroundColor: null,
                color: '#ffffff',
                hoverOpacity: 0.7,
                selectedColor: '#1de9b6',
                enableZoom: true,
                showTooltip: true,
                values: sample_data,
                scaleColors: [ '#1de9b6', '#03a9f5' ],
                normalizeFunction: 'polynomial'
            } );
        } )( jQuery );
    </script>
</head>
<body>
    <div>
        @RenderBody()
    </div>
    @section Scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
</body>
</html>
