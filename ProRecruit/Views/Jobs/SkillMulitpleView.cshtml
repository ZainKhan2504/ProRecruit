@model IEnumerable<ProRecruit.JobSkill>
@{
    ViewBag.Title = "SkillMulitpleView";
    Layout = "~/Views/Shared/_Dashboard.cshtml";
}
<div class="col-sm-8">
    <div class="card border border-primary">
        <div class="card-header">
            <strong class="card-title"><b>Your Skills</b></strong>
        </div>
        <div class="card-body">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">Skill Name</th>
                        <th scope="col">Experience</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@Html.DisplayFor(modelItem => item.Skill.SkillName)</td>
                            <td>@Html.DisplayFor(modelItem => item.SkillExperience.Experience)</td>
                            <td>@Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "btn btn-primary" })</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

